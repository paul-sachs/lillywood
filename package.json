{
  "name": "Lillywood",
  "version": "0.0.1",
  "description": "A virtual office space application. Allows screen sharing and voice chat rooms.",
  "main": "",
  "scripts": {
    "build": "webpack --progress --profile --colors",
    "dev": "webpack-dev-server --progress --profile --colors --hot",
    "kot": "kotatsu serve --index public/index.html -c webpack.config.js --babel ./index.js --mountNode 'app'",
    "test-ava": "ava",
    "test": "karma start --auto-watch"
  },
  "ava": {
    "files": [
      "spec/*.js"
    ],
    "failFast": true,
    "tap": true,
    "require": [
      "babel-register"
    ]
  },
  "repository": {
    "type": "git"
  },
  "keywords": [
    "Screensharing",
    "WebRTC"
  ],
  "author": "Paul Sachs",
  "license": "MIT",
  "dependencies": {
    "ava": "^0.12.0",
    "expect": "^1.14.0",
    "history": "^2.0.0-rc2",
    "imports-loader": "^0.6.5",
    "karma": "^0.13.21",
    "karma-chrome-launcher": "^0.2.2",
    "karma-cli": "^0.1.2",
    "karma-mocha": "^0.2.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "kotatsu": "^0.9.1",
    "lodash": "^3.10.1",
    "mocha": "^2.4.5",
    "peerjs": "^0.3.14",
    "re-base": "^1.5.1",
    "react": "0.14.6",
    "react-dom": "0.14.6",
    "react-router": "^2.0.0-rc5",
    "skylinkjs": "^0.6.7",
    "webrtc-adapter": "^1.0.1"
  },
  "devDependencies": {
    "ava": "^0.12.0",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "babel-preset-stage-0": "^6.3.13",
    "css-loader": "0.23.1",
    "file-loader": "0.8.4",
    "kotatsu": "^0.9.1",
    "react-hot-loader": "1.2.7",
    "react-router": "^1.0.3",
    "style-loader": "0.13.0",
    "url-loader": "0.5.6"
  }
}
